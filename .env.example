# AWS S3 Configuration
AWS_ACCESS_KEY_ID=your_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_secret_access_key_here
S3_BUCKET_NAME=watchme-vault
AWS_REGION=us-east-1

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_anon_key_here

# Device Skip Configuration (Cost Optimization)
# Set SKIP_ENABLED=true to enable processing skip for specific devices/hours
SKIP_ENABLED=false
# Comma-separated device IDs to skip (e.g., 9f7d6e27-98c3-4c19-bdfb-f7fda58b9a93,another-device-id)
SKIP_DEVICE_IDS=
# Comma-separated hours to skip (e.g., 23,0,1,2,3,4,5 for night hours 23:00-05:59)
# Leave empty to NOT skip any hours (must specify hours to enable skipping)
SKIP_HOURS=